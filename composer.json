{
    "name": "liveintent/sdk-php",
    "description": "LiveIntent PHP SDK",
    "keywords": [
        "liveintent",
        "sdk",
        "php"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.4",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "LiveIntent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "lint": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
